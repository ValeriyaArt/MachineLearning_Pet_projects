end(name)\n","      song_link.append(link)\n","    for i in song_link:\n","      page = requests.get(i)\n","      soup = bs(page.text, \"html.parser\")\n","      song = soup.find(class_=\"m207\")\n","      listToStr = ' '.join([str(elem) for elem in song]) \n","      song_text = re.sub('<br/> * Добавил .*.$', '', listToStr)\n","      song_text1 = re.sub('<br/>', '', song_text)\n","      song_text2 = re.sub('\\r', '', song_text1)\n","      song_text3 = re.sub('\\n', '', song_text2)\n","      song_text4 = re.sub('<p>', '', song_text3)\n","      song_text5 = re.sub('<p/>', '', song_text4)\n","      song_texts.append(song_text5)\n","  data = pd.DataFrame(list(zip(song_name, song_link, song_texts)),\n","                      columns = ['Название песни', 'Ссылка на текст', 'Текст'])\n","  return data "],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"KmyFO1nEZdKg"},"source":["korol_i_shut = ['https://www.beesona.ru/songs/korol_i_shut/',\n","                'https://www.beesona.ru/songs/korol_i_shut/p2',\n","                'https://www.beesona.ru/songs/korol_i_shut/p3',\n","                'https://www.beesona.ru/songs/korol_i_shut/p4',\n","                'https://www.beesona.ru/songs/korol_i_shut/p5',\n","                'https://www.beesona.ru/songs/korol_i_shut/p6']\n","korol_i_shut_data = get_data(korol_i_shut)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"GPzeGRb7ZuEY"},"source":["ddt = ['https://www.beesona.ru/songs/ddt/',\n","       'https://www.beesona.ru/songs/ddt/p2',\n","       'https://www.beesona.ru/songs/ddt/p3',\n","       'https://www.beesona.ru/songs/ddt/p4',\n","       'https://www.beesona.ru/songs/ddt/p5',\n","       'https://www.beesona.ru/songs/ddt/p6']\n","ddt_data = get_data(ddt)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"KNxqjeDAdAmE"},"source":["zemfira = ['https://www.beesona.ru/songs/zemfira/',\n","           'https://www.beesona.ru/songs/zemfira/p2',\n"